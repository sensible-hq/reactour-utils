  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "portal",
    "utilities",
    "observables",
    "resize observer",
    "mutatio observer",
    "smooth scroll",
    "get bounding client rect"
  ]
}
